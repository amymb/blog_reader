<%=form_for @user do |f|%>
  <%=f.label :username%>
  <%=f.text_field :username%>
  <%=f.label :email%>
  <%=f.email_field :email%>
  <%=f.label :wordpress_username%>
  <%=f.text_field :wordpress_username%>
  <%=f.label :full_name%>
  <%=f.text_field :full_name%>
  <%=f.label :password%>
  <%=f.password_field :password%>
  <%=f.label :password_confirmation%>
  <%=f.password_field :password_confirmation%>
  <%=f.submit "Update Profile"%>
<%end%>
